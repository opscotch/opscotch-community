{
    "configVersionId": "234",
    "routes": [
        {
            "name": "Get contracts list",
            "steps": [
                {
                    "type": "scripted",
                    "routeName": "get-contracts-list",
                    "timer": "fixedRate=true&period=600000",
                    "urlGenerator": {
                        "resource": "/general/standard-url-generator.js",
                        "data": {
                            "path": "/contracts?_limit=100&_filters=contract_type(17)",
                            "host-ref": "accelo"
                        }
                    },
                    "resultsProcessor": {
                        "resource": "/general/standard-forwarder.js",
                        "data": {
                            "processroute": "accelo-get-periods"
                        }
                    }
                },
                {
                    "type": "scripted-split-aggregate",
                    "routeName": "accelo-get-periods",
                    "splitGenerator": {
                        "resource": "/accelo/contracts-array-aggregator-split.js"
                    },
                    "urlGenerator": {
                        "resource": "/accelo/periods-urlgenerator.js",
                        "data": {
                            "host-ref": "accelo"
                        }
                    },
                    "resultsProcessor": {
                        "resource": "/accelo/periods-resultsprocessor.js",
                        "data": {
                            "prefix": "on-budget-"
                        }
                    }
                }
            ]
        }
    ]
}
