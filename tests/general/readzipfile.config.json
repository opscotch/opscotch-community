{
  "workflows": [
    {
      "name": "read zip file",
      "steps" : [
        {
          "stepId": "readzip",
          "type": "httpFile",
          "trigger": {
            "http" : {
              "server": "staticFileServer",
              "path" : ".*",
              "method" : "get"
            }
          },
          "resultsProcessor": {
            "script": "context.setProperty('path', JSON.parse(context.getBody()).path);"
          }
        }
      ]
    }
  ]
}